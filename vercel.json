{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/admin-api/(.*)", "dest": "/api/admin-proxy/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/auth", "dest": "/auth/index.html" },
    { "src": "/auth/(.*)", "dest": "/auth/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
