{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/admin-api/:path*",
      "destination": "/api/admin-proxy/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
